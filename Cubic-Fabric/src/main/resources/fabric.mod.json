  "description": "Adds support for creating games using server side plugins.",
  "authors": [
    "DaylightNebula"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },
  "license": "CC0-1.0",
  "icon": "icon.png",
  "environment": "*",
